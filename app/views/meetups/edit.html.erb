<h1>Edit Meetup</h1>
<%= link_to "Back to meetups", meetups_path %>

<%= form_for @meetup, url: meetup_path(@meetup), method: :patch do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :date %>
    <%= f.date_select :date %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.submit "Update Meetup" %>
  </div>
<% end %>
